<!DOCTYPE html>
<html>
<head>
  <title>Attendance</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= stylesheet_link_tag "jquery-ui-1.8.21.custom","application" %>
  <%= javascript_include_tag "jquery-ui-1.8.21.custom.min.js","application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if session[:user] %>
    <%= "<b style=color:#FFA500>Welcome #{session[:user].email}!</b>".html_safe%> &nbsp;  <%= link_to "Logout", logout_path %>

<% end %>
<%= flash[:notice] %>
<%= yield %>

</body>
</html>
